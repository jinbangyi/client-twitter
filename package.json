{
    "name": "@elizaos/client-twitter",
    "version": "0.25.6-alpha.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "types": "./dist/index.d.ts"
    },
    "files": [
        "dist",
        "src",
        "*.ts",
        "*.json"
    ],
    "dependencies": {
        "@elizaos/core": "0.1.9",
        "agent-twitter-client": "github:jinbangyi/agent-twitter-client#main",
        "axios": "^1.7.9",
        "discord.js": "14.16.3",
        "glob": "11.0.0",
        "https-proxy-agent": "^7.0.6",
        "pino": "^9.6.0",
        "prom-client": "^15.1.3",
        "sharp": "0.33.5",
        "zod": "3.24.1"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.12",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.11",
        "@typescript-eslint/eslint-plugin": "6.21.0",
        "@typescript-eslint/parser": "6.21.0",
        "@vitest/coverage-v8": "1.1.3",
        "eslint": "8.57.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.2.3",
        "jest": "^29.7.0",
        "prettier": "3.5.1",
        "ts-node": "^10.9.2",
        "tsup": "8.3.5",
        "typescript": "5.6.3",
        "vitest": "1.6.1"
    },
    "scripts": {
        "build": "tsup tsup.config.ts --format esm --dts",
        "build2": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "start": "node dist/index.js",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\" \"debug/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "build:debug": "tsup debug/*.ts",
        "debug": "tsup debug/debug.ts && node dist/debug.js",
        "postinstall": "pnpm run build2"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
